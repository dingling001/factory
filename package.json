{
  "name": "apicloud-polyfill-sample",
  "version": "0.0.1",
  "description": "apicloud-polyfill,以模块化方式开发 APICloud App!",
  "main": "index.js",
  "repository": "http://www.apicloud.com",
  "scripts": {
    "webpack": "webpack",
    "wifiStart": "apicloud wifiStart --port 8686",
    "wifiSync": "apicloud wifiSync --project ./ --updateAll false",
    "wifiAllSync": "apicloud wifiSync --project ./ --updateAll true",
    "wifiLog": "apicloud wifiLog --port 8686",
    "wifistop": "apicloud wifiStop --port 8686"
  },
  "author": "APICloud",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "glob": "^7.1.1",
    "moment": "^2.24.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "apicloud-polyfill": "^0.1.1",
    "apicloud-tools-core": "^0.1.0"
  }
}
